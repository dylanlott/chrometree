{
    "manifest_version": 2,
    "name": "TreeMark",
    "description": "See and organize your bookmarks as trees",
    "version": "1.0",
    "icons": {
    	"16" : "icon_16.png",
    	"48" : "icon_48.png",
    	"128": "icon_128.png"
    },
    "chrome_url_overrides" : {
    	"newtab": "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
    	"bookmarks",
    	"tabs",
        "unlimitedStorage",
        "management"
    ]
}